generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  username String @unique
  password String

  firstName String
  lastName  String
  age       Int
  email     String? @unique

  Address Address[]
}

model Address {
  id String @id @default(uuid())

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  number Int
  street String
  city   String

  User   User?   @relation(fields: [userId], references: [id])
  userId String?
}
